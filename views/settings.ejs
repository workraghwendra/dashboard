		            <!--Begin::Section-->
<div class="row">
	<div class="col-md-12">
		<!--begin:: Widgets/Quick Stats-->
        <div class="row">
                <div class="col-xl-3 col-lg-4">
                        <div class="m-portlet m-portlet--full-height ">
                            <div class="m-portlet__head text-center">
                                <div class="m-portlet__head-title" style="padding-top:30px;">
                                    <h6>Two Factor Authentication</h6>
                                </div>
                            </div>
                            <div class="m-portlet__body"><br>
                                <div class="col-lg-8 col-xl-4;" style="margin-left:50px;">
                                        <input type="hidden" name="builder[layout][header][self][fixed][desktop]" value="false">
                                        <span class="m-switch m-switch--icon-check">
                                            <label>
                                                <input id="2fa" type="checkbox" name="builder[layout][header][self][fixed][desktop]" onclick="tfa()" <%= (tfastatus==true) ? "checked":"" %> > 
                                                <span></span>
                                            </label>
                                        </span>
                                </div><br><br>
                                <p>Click to enable/disable two factor authentication</p>
                            </div>  
                        </div>
                    </div>
                <div class="col-xl-9 col-lg-8">
                    <div class="m-portlet m-portlet--full-height m-portlet--tabs  ">
                        <div class="m-portlet__head">
                            <div class="m-portlet__head-tools">
                                <ul class="nav nav-tabs m-tabs m-tabs-line   m-tabs-line--left m-tabs-line--primary" role="tablist">
                                    <li class="nav-item m-tabs__item">
                                        <a class="nav-link m-tabs__link active" data-toggle="tab" href="#m_user_profile_tab_1" role="tab" aria-expanded="true">
                                        <i class="flaticon-share m--hide"></i>
                                            Edit Profile
                                        </a>
                                    </li>
                                    <li class="nav-item m-tabs__item">
                                        <a class="nav-link m-tabs__link" data-toggle="tab" href="#m_user_profile_tab_2" role="tab" aria-expanded="false">
                                            Settings
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="tab-content">
                            <div class="tab-pane active" id="m_user_profile_tab_1" aria-expanded="true">
                                <div id="changebasicinfo">
                                <div class="m-form m-form--fit m-form--label-align-right">
                                    <div class="m-portlet__body">
                                        <div class="form-group m-form__group row">
                                            <label for="example-text-input" class="col-xl-2 col-lg-2 col-form-label">Full Name</label>
                                            <div class="col-xl-8 col-lg-8">
                                                    <div class="input-group">
                                                <input type="text" name="name" class="form-control m-input" placeholder="New Name" value=" <%= userName %>" id="changedname"
                                                aria-describedby="name-error" disabled>
                                                <button id="nameeditable" onclick="editname()"><i class="fa fa-pencil"></i></button>
                                            </div>
                                            </div>
                                        </div>
                                        <div class="form-group m-form__group row">
                                            <label for="example-text-input" class="col-xl-2 col-lg-2 col-form-label">Email Address</label>
                                            <div class="col-xl-8 col-lg-8">
                                                <input type="email" name="email" class="form-control m-input" placeholder="New E-Mail"  value=" <%= userID %>" id="changedemail"
                                                aria-describedby="email-error" disabled>
                                                <!-- <button onclick="editemail()"><i class="fa fa-pencil"></i></button> -->
                                            </div>
                                        </div>
                                        <div class="form-group m-form__group row">
                                            <label for="example-text-input" class="col-xl-2 col-lg-2 col-form-label">Mobile No.</label>
                                            <div class="col-xl-8 col-lg-8">
                                                    <div class="input-group">
                                                <input type="string" name="phone" class="form-control m-input" placeholder="New Number" id="changednumber" value=" <%= userPhone %>"
                                                aria-describedby="phone-error" disabled>
                                                
                                                        <button onclick="editnumber()"><i class="fa fa-pencil"></i></button>
                                                
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group m-form__group row">
                                            <div class="col-xl-2 col-lg-2">
                                            </div>
                                            <div class="col-xl-10 col-lg-10">
                                                <button type="reset" class="btn btn-accent m-btn m-btn--air m-btn--custom" onclick="editinformation()" id="savebasicinfo" disabled>Update</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="m_user_profile_tab_2" aria-expanded="false">
                                    
                                <div class="m-form m-form--fit m-form--label-align-right">
                                    <div class="m-portlet__body">
                                    
                                            <form id="changepassword">
                                                    <div class="form-group m-form__group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Current Password:</label>
                                                        <div class="col-xl-9 col-lg-9">
                                                            <input type="Password" name="email" class="form-control m-input" placeholder="Current Password" id="currentpass" aria-describedby="email-error" onkeyup='myfunction0()'>
                                                        </div>
                                                    </div>
                                                    <div class="form-group m-form__group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">New Password:</label>
                                                        <div class="col-xl-9 col-lg-9">
                                                            <input type="Password" name="email" class="form-control m-input" placeholder="New Password" id="newpass" aria-describedby="email-error">
                                                        </div>
                                                            <div class="col-xl-12 col-lg-12 text-error-text text-danger small" style="text-align:right;"><span id="newpasserror"></span></div>
                                                    </div>
                                                    <div class="form-group m-form__group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label">Confirm Password:</label>
                                                        <div class="col-xl-9 col-lg-9">
                                                            <input type="Password" name="email" class="form-control m-input" placeholder="Verify New Password" id="vernewpass" aria-describedby="email-error" onkeyup='myfunction0()'>
                                                        </div>
                                                            <div class="col-xl-12 col-lg-12 text-error-text text-danger small" style="text-align:right;" ><span id="vernewpasserror"></span></div>
                                                        </div>
                                                    <div class="form-group m-form__group row">
                                                        <label class="col-xl-3 col-lg-3 col-form-label"></label>
                                                        <div class="col-xl-2 col-lg-2">
                                                            <button style="color:#189dd9" class="btn btn-primary2">Save</button>
                                                        </div>
                                                    </div>
                                                </form>
                                    
                                    </div>
                                </div>
                            
                            </div>
                        </div>
                    </div>
                </div>
            </div>
</div>

</div>

