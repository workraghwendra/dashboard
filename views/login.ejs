									<input id="phone" name="phone" placeholder="<%= __('phonNuPassPlacHol') %>" type="number" class="form-control" value=""  onkeyup='myfunction()'>
									<div class="text-error-text float-right text-danger small" id="NumberError"></div>                                        
								</div>
								<div class="form-group m-form__group">
