        <div class="m-content">
            <div class="m-portlet m-portlet--responsive-mobile">
                <div class="m-portlet m-portlet--responsive-mobile">
                    <div class="m-portlet__head">
                        <div class="m-portlet__head-caption">
                            <div class="m-portlet__head-title">
                                <span class="m-portlet__head-icon">
                                    <i class="flaticon-music-2"></i>
                                </span>
                                <h3 class="m-portlet__head-text">
                                    USER STATS
                                </h3>
                            </div>
                        </div>
                    </div>
                    <div class="m-portlet__body">
                        <!--begin::Section-->
                        <div class="m-section__content ">
                                <div class="row ">
                                <div class="col-md-3 col-sm-3 col-xs-6 tile_stats_count text-center line">
                                    <span class="count_top ">
                                        <h5>Total User</h5>
                                    </span>
                                    <div id="totaluser" class="count " style="font-size:125%">
                                        <%= userpurcoins %>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-3 col-xs-6 tile_stats_count text-center line ">
                                    <span class="count_top ">
                                        <h5>Total Active User</h5>
                                    </span>
                                    <div id="totalactiveuser"class="count " style="font-size:125%">
                                        <span id="valueoftoken">
                                        <%= userrefcoins %>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-3 col-xs-6 tile_stats_count text-center line ">
                                    <span class="count_top ">
                                        <h5>Total Inactive User</h5>
                                    </span>
                                    <div id="totalinactiveuser"class="count " style="font-size:125%">
                                        <span id="valueoftoken">
                                        <%= userrefcoins %>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-3 col-xs-6 tile_stats_count text-center ">
                                    <span class="count_top ">
                                        <h5>Total User Register In Last 24 Hrs</h5>
                                    </span>
                                    <div id="24hrs"class="count " style="font-size:125%">
                                        <span>
                                        <%= res %>
                                        </span>
                                    </div>
                                </div>
                                </div>
                            </div>
                            
                        <!--end::Section-->
                    </div>
                    <div class="col-md-12">
                            <canvas id="meChart" style="width:320px; height:100px;" ></canvas>
                            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
                           <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.20.1/locale/mr.js"></script>-->
                            <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>
                            <script>
                               
                                var ctx = document.getElementById('meChart').getContext('2d');
                                var day = []
                                var user = []
                                var days = []
                                var jsonData = $.ajax({
                                    url: '/api/fetchd',
                                    dataType: 'json',
                                }).done(function(response) {
                                  //  console.log(response)
                                    var length = response.response.length
                                   // for (i = 0; i < length; i++) {
                                    //     var date = response.response[i]._id
                                     
                                    //     days.push(moment(date).format('D  YYYY'))
                                    // }
                                //    console.log(days)
                                    for (n = 0; n < length; n++) {
                                        user.push(response.response[n].count)
                                       days.push(response.response[n]._id.day)
                                      //day.push(response.response[n]._id.month)
                                    }
                                  //  console.log(days)
                                })
                                $(document).ready(function () {
                                var chart = new Chart(ctx, {
                                    type: 'line',
                                    data: {
                                        labels: days,
                                        datasets: [{
                                            label: "User Stats",
                            
                                            data: user,
                                            backgroundColor: 'rgb(255, 0, 255)'
                            
                                        }]
                                    },
                                    options: {}
                                });
                            })
                            </script>
                            </div>
                </div>  
               
            </div>
          
         
        </div>

