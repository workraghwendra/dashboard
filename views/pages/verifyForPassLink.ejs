    <script src="/javascripts/js/login.js" type="text/javascript"></script>
    <script>
        toastr.options = {
            "closeButton"     : true,
            "debug"         : false,
            "newestOnTop"     : true,
            "progressBar"     : true,
            "positionClass"     : "toast-top-right",
            "preventDuplicates"   : true,
            "showDuration"      : "300",
            "hideDuration"      : "1000",
            "timeOut"       : "5000",
            "extendedTimeOut"   : "3000",
            "showEasing"      : "swing",
            "showMethod"      : "fadeIn",
            "tapToDismiss"      : false
        }
        </script>
       
